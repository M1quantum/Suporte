    <table>
        <caption>Título da tabela</caption>
        <thead>
            <!-- Cabeçalho da tabela -->
            <tr>
                <th>Col 1</th> <th>Col 2</th> <th>Col 3</th>
            </tr>
        </thead>
        <tbody>
            <!-- Corpo da tabela -->
            <tr>
                <td>11</td> <td>12</td> <td>13</td>
            </tr>
            <tr>
                <td>21</td> <td>22</td> <td>23</td>
            </tr>
            <tr>
                <td>31</td> <td>32</td> <td>33</td>
            </tr>
        </tbody>
        <tfoot>
            <!-- rodapé da tabela -->
            <tr>
                <td>fim 1</td> <td>fim 2</td> <td>fim 3</td>
            </tr>
        </tfoot>        
    </table>


